<!DOCTYPE html>
<html>

<head>
  <title>ShopifyChallengeImageRepository</title>
  <%= csrf_meta_tags %>
  <%= csp_meta_tag %>

  <meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no">
  <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
  <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
</head>

<body>
  <%= render 'layouts/nav' %>
  <div class="container">
    <% if notice %>
    <div class="notice alert alert-primary mt-3 col-md-4 offset-md-4 col-12"><%= notice %></div>
    <% end %>

    <% if alert %>
    <div class="alert alert alert-danger mt-3 col-md-4 offset-md-4 col-12"><%= alert %></div>
    <% end%>
    <%= yield %>
  </div>
</body>

</html>